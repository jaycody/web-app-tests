    {
      "type": "pause",
      "waitTime": "1000"
    },
    {
      "type": "verifyTextPresent",
      "text": "Tell us about your home!"
    }
  ],
  "data": {
    "configs": {},
    "source": "none"
  },
  "inputs": [],
  "timeoutSeconds": 60
